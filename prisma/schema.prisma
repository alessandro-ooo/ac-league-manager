// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model User {
    id String @id
    name String @unique
    files File[]
}

model File {
    uid String
    name String @unique
    users User @relation(fields: [uid], references: [id])
    texture String @unique
}

model Car {
    name String @unique
    model String @unique
}